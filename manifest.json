{
	"name": "BackStop",
	"description": "Stop Backspace returning to previous page.",
	"version": "1.2",
	"update_url": "http://cranman.uwcs.co.uk/chrome/updates.xml",
	"icons": {
		"16": "logo16.png",
		"48": "logo48.png",
		"128": "logo128.png"
	},
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": ["jquery.js", "jquery.hotkeys.js", "content.js"],
			"exclude_globs":  ["https://plus.google.com/*"]
		}
	]
}